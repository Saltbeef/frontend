# House Analysis Agent Service - Environment Variables
# Copy this file to .env and fill in your actual values

# Apify Configuration
APIFY_API_TOKEN=your_apify_token_here
APIFY_DATASET_ID=your_dataset_id_here

# LLM Provider API Keys (choose one or both)
ANTHROPIC_API_KEY=sk-ant-api03-...
OPENAI_API_KEY=sk-...

# GitHub Configuration (for local testing of workflow triggers)
GITHUB_TOKEN=ghp_your_personal_access_token
GITHUB_REPO=Saltbeef/frontend

# Optional: Override default settings
# LLM_PROVIDER=claude  # Options: mock, claude, openai
# RULES_VERSION=latest  # Or specific version like v1.0.0
